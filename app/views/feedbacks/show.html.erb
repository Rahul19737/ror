<div class="card text-center shadow-lg mb-5">
  <div class="card-header">
    <%= @feedback.created_at %>
  </div>
  <div class="card-body">
    <p class="card-text"> <%= @feedback.feedback %> </p>
  </div>
  <div class="card-footer">
  <%= link_to 'Edit', edit_feedback_path(@feedback), class:'btn btn-primary' %>
  <%= link_to 'Back', feedbacks_path, class: 'btn btn-secondary' %>
  </div>
</div>
